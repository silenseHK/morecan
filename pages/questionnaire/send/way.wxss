.container{
  background: #f5f5f6;
  position: relative;
  padding: 0 138rpx;
}
.send-title{
  padding-top: 120rpx;
  font-size: 40rpx;
}
.way-container{
  margin-top: 130rpx;
}
.finish-btn{
  height: 92rpx;
  width: 450rpx;
  line-height: 92rpx;
  background: rgb(240, 196, 159);
  border-radius: 100rpx;
  text-align: center;
  line-height: 100rpx;
  font-size: 36rpx;
}
.other{
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  font-weight: normal;
  z-index: 9;
  padding: 90rpx 0;
}
.other::after{
  position: absolute;
  content: '';
  border: 2rpx solid #666;
  height: 50rpx;
  width: 50rpx;
  border-radius: 10rpx;
  background: #f5f5f6;
  transform: rotate(45deg);
  z-index: -1;
}
.other::before{
  position: absolute;
  content: '';
  height: 0;
  width: 230rpx;
  border-bottom: 2rpx dashed #666;
  z-index: -2;
}
.way-desc{
  color: #333;
  margin-top: 48rpx;
  text-align: center;
  font-size: 24;
}

